<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  


  <!--============================== Beds ==============================-->

 <ThingDef ParentName="BedWithQualityBase">
    <defName>MFFG_OperatingBed</defName>
    <label>operating bed</label>
    <description>A bed specially designed for use in operating rooms. Adjustable for a patient and festooned with built-in equipment, it improves surgical outcomes compared to conventional beds.</description>
    <graphicData>
      <texPath>Building/OperatingBed/MFFG_OperatingBed</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <castEdgeShadows>True</castEdgeShadows>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>2800</WorkToBuild>
      <Mass>35</Mass>
      <Flammability>1.0</Flammability>
	  <BedRestEffectiveness>0.1</BedRestEffectiveness>
      <Comfort>0.20</Comfort>
      <SurgerySuccessChanceFactor>1.5</SurgerySuccessChanceFactor>
      <BirthRitualQualityOffset MayRequire="Ludeon.RimWorld.Biotech">15</BirthRitualQualityOffset>
    </statBases>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>VitalsMonitor</li>
		  <li>MFFG_Surgical_Instruments</li>
        </linkableFacilities>
      </li>
    </comps>
    <size>(1,2)</size>
    <costList>
      <Steel>80</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <building>
      <bed_showSleeperBody>False</bed_showSleeperBody>
      <bed_healPerDay>10</bed_healPerDay>
      <bed_defaultMedical>true</bed_defaultMedical>
      <buildingSizeCategory>Small</buildingSizeCategory>
    </building>
    <fillPercent>0.4</fillPercent>
    <pathCost>42</pathCost>
    <leaveResourcesWhenKilled>True</leaveResourcesWhenKilled>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <designationHotKey>Misc5</designationHotKey>
    <researchPrerequisites>
      <li>HospitalBed</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <techLevel>Industrial</techLevel>
    <uiIconScale>0.90</uiIconScale>
  </ThingDef>
 
  
  <!--============================== Storage ==============================-->

  <ThingDef ParentName="FurnitureBase">
    <defName>MFFG_SmallCrate</defName>
    <label>small crate</label>
    <description>A small triple-stack shelf that holds three times as much as empty ground. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nSince shelf space is limited, shelves cannot hold chunks, buildings, plants and large corpses.</description>
    <graphicData>
      <texPath>Building/MFFG_Crate</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <uiIconScale>0.8</uiIconScale>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints> 
      <Mass>4</Mass>
      <WorkToBuild>250</WorkToBuild>
	  <Flammability>1.0</Flammability>
      <Beauty>0.5</Beauty>
    </statBases>
    <size>(1,1)</size>
    <building>
	  <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <maxItemsInCell>4</maxItemsInCell>
      <blueprintClass>Blueprint_Storage</blueprintClass>
      <!-- <paintable>true</paintable> -->
	  <storageGroupTag>Shelf</storageGroupTag>
      <relatedBuildCommands>
        <li>Shelf</li>
      </relatedBuildCommands>
	  <fixedStorageSettings>
        <filter>
          <disallowNotEverStorable>true</disallowNotEverStorable>
          <categories>
            <li>Root</li>
          </categories>
          <disallowedCategories>
            <li>Chunks</li>
            <li>Plants</li>
            <li>Buildings</li>
          </disallowedCategories>
          <disallowedThingDefs MayRequire="Ludeon.RimWorld.Biotech">
            <li MayRequire="Ludeon.RimWorld.Biotech">Wastepack</li>
          </disallowedThingDefs>
          <specialFiltersToDisallow>
            <li>AllowLargeCorpses</li>
          </specialFiltersToDisallow>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Preferred</priority>
        <filter>
          <categories>
            <li>Foods</li>
            <li>Manufactured</li>
            <li>ResourcesRaw</li>
            <li>Items</li>
            <li>Weapons</li>
            <li>Apparel</li>
            <li>BodyParts</li>
          </categories>
        </filter>
      </defaultStorageSettings>
    </building>
	<thingClass>Building_Storage</thingClass>
	<altitudeLayer>Building</altitudeLayer>
	<passability>PassThroughOnly</passability>
    <fillPercent>0.4</fillPercent>
    <castEdgeShadows>true</castEdgeShadows>
	<rotatable>false</rotatable>
    <pathCost>50</pathCost>
    <designationHotKey>Misc12</designationHotKey>
	<inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
	<costList>
      <Steel>30</Steel>
    </costList>
    <staticSunShadowHeight>0.5</staticSunShadowHeight>
    <surfaceType>Item</surfaceType>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
  </ThingDef>

    

	


  
